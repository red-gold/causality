{
    "workspaces": [
        "packages/*",
        "notebooks/",
        "causal-doc/",
        "web-app-demos/*"
    ], 
    "name": "causal-net", 
    "license": "MIT", 
    "author": "nguyenhuynh200188@gmail.com", 
    "version": "0.1.0", 
    "private": true, 
    "dependencies": {
        "causal-net": "*", 
        "causal-net.deployment": "*", 
        "causal-net.storage": "*", 
        "causal-net.layer": "*", 
        "causal-net.core": "*", 
        "causal-net.datasets": "*", 
        "causal-net.memcache": "*", 
        "causal-net.preprocessing": "*", 
        "causal-net.optimizers": "*", 
        "causal-net.utils": "*", 
        "causal-net.sampling": "*", 
        "causal-net.log": "*"
    }, 
    "scripts": {
        "build:prod": "webpack --config webpack.prod.babel.js", 
        "build:demos": "cd web-app-demos;build:demos;cp -r ./demo_dist ../doc_dist/prebuilt-demos;",
        "start:doc": "rimraf ./doc_dist/; esdoc -c esdoc.config.json; cd doc_dist/; xdg-open http://127.0.0.1:8080/index.html; simple_server;", 
        "build:doc": "rimraf ./doc_dist/; esdoc -c esdoc.config.json;", 
        "build:clean": "rimraf dist", 
        "test": "jest", 
        "test:report": "jest --coverage", 
        "build:dev": "webpack --config webpack.dev.babel.js"
    }, 
    "devDependencies": {
        "eslint-loader": "^2.1.1", 
        "@babel/register": "^7.0.0", 
        "webpack-cli": "^3.1.2", 
        "esdoc-standard-plugin": "^1.0.0", 
        "@babel/plugin-proposal-object-rest-spread": "^7.2.0", 
        "@babel/preset-env": "^7.2.3", 
        "webpack-merge": "^4.1.5", 
        "html-webpack-plugin": "^3.2.0", 
        "webpack": "^4.28.2", 
        "babel-loader": "^8.0.4", 
        "eslint": "^5.12.0", 
        "@babel/types": "^7.2.2", 
        "@babel/plugin-syntax-dynamic-import": "^7.2.0", 
        "webpack-dev-server": "^3.1.13", 
        "@babel/plugin-transform-runtime": "^7.2.0", 
        "@babel/core": "^7.2.2"
    }
}